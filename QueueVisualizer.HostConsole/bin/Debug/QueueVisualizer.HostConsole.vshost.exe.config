<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <services>
      <service name="QueueVisualizer.SoapService.QueueService" behaviorConfiguration="QueueVisualizerBehavior">
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:9001/QueueVisualizer"/>
            <add baseAddress="net.tcp://localhost:9002/QueueVisualizer"/>
          </baseAddresses>
        </host>
        <endpoint address="http://localhost:9001/QueueVisualizer" binding="basicHttpBinding" contract="QueueVisualizer.SoapService.IQueueServiceContract"/>
        <endpoint address="net.tcp://localhost:9002/QueueVisualizer" binding="netTcpBinding" contract="QueueVisualizer.SoapService.IQueueServiceContract"/>
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
        <endpoint address="mex" binding="mexTcpBinding" contract="IMetadataExchange"/>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="QueueVisualizerBehavior">
          <serviceMetadata httpGetEnabled="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>